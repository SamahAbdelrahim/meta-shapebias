@article{gentner_1978,
	abstract = {The acquisition of verb meaning is very different from the acquisition of simple noun (concrete and proper) meaning. Verbs are slower to be acquired than simple nouns and are used by children and adults with greater breadth of application. Finally, a componential account of meaning appears to fit the acquisition of verb meaning somewhat better than it does that of simple nouns. These differences reflect an important difference in the kind of meaning conveyed by verbs and simple nouns: Simple nouns refer to real-world entities, and verbs convey relationships among entities. The theories of E. V. Clark (1973) and K. Nelson (see record 1974-32036-001) are prominently considered. (38 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	author = {Gentner, Dedre},
	date-modified = {2023-12-14 22:27:19 -0800},
	doi = {10.2307/1128738},
	file = {Snapshot:files/190/1979-28248-001.html:text/html;Submitted Version:files/191/Gentner - 1978 - On relational meaning The acquisition of verb mea.pdf:application/pdf},
	issn = {1467-8624},
	journal = {Child Development},
	keywords = {Language Development, Nouns, Word Meaning, Verbs},
	note = {Place: United Kingdom Publisher: Blackwell Publishing},
	number = {4},
	pages = {988--998},
	shorttitle = {On relational meaning},
	title = {On relational meaning: {The} acquisition of verb meaning},
	volume = {49},
	year = {1978},
	bdsk-url-1 = {https://doi.org/10.2307/1128738}}

