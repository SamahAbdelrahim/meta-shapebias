	issn = {1548-7660},
	journal = {Journal of Statistical Software},
	language = {en},
	month = aug,
	pages = {1--48},
	title = {Conducting {Meta}-{Analyses} in {R} with the metafor {Package}},
	url = {https://doi.org/10.18637/jss.v036.i03},
	urldate = {2023-11-10},
