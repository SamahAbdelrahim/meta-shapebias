	keywords = {Buoyancy, Children, Education, Psychology, Seven-year-olds, Three-year-olds},
	language = {English},
	note = {ISBN: 9780599858954},
	school = {University of California, Berkeley},
	shorttitle = {"{Its} size is really wood"},
	title = {"{Its} size is really wood": {The} development of buoyancy and material kind understanding in children between three and seven years of age},
	type = {Ph.{D}.},
